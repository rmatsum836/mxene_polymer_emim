constraints         = none
;constraint-algorithm = LINCS
;lincs_iter          = 1
;lincs_order         = 4
integrator          = md
nsteps              = 50
dt                  = 0.001

nstxout             = 1
nstvout             = 0
nstenergy           = 1
nstlog              = 1

cutoff-scheme       = Verlet
ns_type             = grid
nstlist             = 10
rcoulomb            = 1.2
rvdw                = 1.2

coulombtype         = PME
fourierspacing      = 0.16
ewald-geometry      = 3d

gen_vel             = no

tcoupl              =  no
;tc-grps             = System
;tau_t               = 0.5
;ref_t               = 298

pcoupl              = no

comm-mode           = None

pbc                 = xyz
periodic-molecules  = yes
